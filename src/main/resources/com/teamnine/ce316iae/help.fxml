<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>

<VBox spacing="10" alignment="top_center" xmlns:fx="http://javafx.com/fxml" fx:controller="com.teamnine.ce316iae.controllers.HelpController"
      minHeight="800" minWidth="1200" maxHeight="800" maxWidth="1200" style="-fx-background-color: #f2f2f2;">
    <TextArea fx:id="helpTextArea" editable="false" wrapText="true" prefWidth="1150" prefHeight="700" style="-fx-font-size: 16px; -fx-padding: 10;">
    </TextArea>
    <Button text="Back" onAction="#goBack" style="-fx-background-color: #0078D7; -fx-text-fill: white; -fx-font-size: 16px; -fx-pref-width: 200; -fx-pref-height: 40;"/>
</VBox>
